        <geom class="collision" type="cylinder" size="0.03 0.2" pos="-0.4 -0.7 0.58"  rgba="0.266754 0.174546 0.0644028 1" />
        <geom class="collision" type="cylinder" size="0.03 0.2" pos="0.4 0.7 0.58"  rgba="0.266754 0.174546 0.0644028 1" />
        <geom class="collision" type="cylinder" size="0.03 0.2" pos="-0.4 0.7 0.58"  rgba="0.266754 0.174546 0.0644028 1" />
        <geom class="visual" type="mesh" mesh="longtable_mesh" material="matmarble" />
      </body>
    </worldbody>
</mujoco>
